{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-responsive-carousel": "^3.1.43",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.3",
    "react-vanilla-tilt": "^1.0.0",
    "vanilla-tilt": "^1.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
